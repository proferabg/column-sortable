{
    "name": "proferabg/sortable-link",
    "description": "Package for handling column sorting links for laravel-query-builder",
    "version": "0.1",
    "keywords": [
        "sortable",
        "sorting",
        "column",
        "sort",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Brenton Profera",
            "email": "brent@prgm.in",
            "role": "Developer and maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5.10|^10.5",
        "orchestra/testbench": "^5.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "proferabg\\SortableLink\\": "src/SortableLink/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "proferabg\\SortableLink\\SortableLinkServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
